ul.nav.nav-list
    each value, key in theme.menu
        li.nav-list-item
            - if (!value) return null
            - var re = /^(http|https):\/\/*/gi;
            - var tar = re.test(value) ? "_blank" : "_self"
            if value.indexOf('.xml') == -1
                a.nav-list-link(href=url_for(value) target=tar)
                    != key.toUpperCase()
            else
                a.nav-list-link(href=url_for(value) target=tar data-barba-prevent)
                    != key.toUpperCase()
